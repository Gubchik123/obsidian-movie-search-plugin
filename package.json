{
	"version": "1.2.1",
	"name": "movie-search",
	"description": "This is a plugin to help you create movie notes.",
	"homepage": "https://github.com/Gubchik123/obsidian-movie-search-plugin/",
	"main": "main.js",
	"standard-version": {
		"t": ""
	},
	"keywords": [
		"movie",
		"movie note",
		"obsidian",
		"plugin"
	],
	"author": "Gubchik123",
	"license": "MIT",
	"engines": {
		"node": ">= 16",
		"yarn": ">= 1.22",
		"npm": "please use yarn"
	},
	"scripts": {
		"dev": "node --experimental-json-modules esbuild.config.mjs",
		"format": "prettier --write \"**/*.ts\"",
		"format:check": "prettier --check \"**/*.ts\"",
		"lint": "yarn format:check && eslint . --ext .ts && tsc --noEmit -skipLibCheck",
		"lint:fix": "yarn format && eslint . --ext .ts --fix",
		"build": "yarn run lint && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"devDependencies": {
		"@popperjs/core": "^2.11.5",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"@typescript-eslint/parser": "^5.2.0",
		"builtin-modules": "^3.2.0",
		"esbuild": "0.13.12",
		"eslint": "^8.22.0",
		"gh-pages": "^4.0.0",
		"husky": "^8.0.1",
		"obsidian": "latest",
		"prettier": "^2.7.1",
		"standard-version": "^9.3.2",
		"ts-node": "^10.9.1",
		"tslib": "2.3.1",
		"typescript": "4.4.4"
	},
	"packageManager": "yarn@1.22.19",
	"dependencies": {
		"languagedetect": "^2.0.0"
	}
}
